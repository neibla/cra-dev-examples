{
  "name": "cra-examples",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.4.0",
    "redbird": "^0.10.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-https-custom": "HTTPS=true SSL_CRT_FILE=./certs/example.com+3.pem SSL_KEY_FILE=./certs/example.com+3-key.pem HOST=example.com react-scripts start",
    "start-https-local": "HTTPS=true react-scripts start",
    "start-https-proxy": "node proxy.js & (BROWSER='launch.js' URL_TO_OPEN='https://example.com' react-scripts start)",
    "start-https-proxy2": "ssl-proxy -cert ./certs/example.com+3.pem -key ./certs/example.com+3-key.pem -from 0.0.0.0:443 -to 127.0.0.1:3000 & (BROWSER='launch.js' && URL_TO_OPEN='https://example.com' && react-scripts start)",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
